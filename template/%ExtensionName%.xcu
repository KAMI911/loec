<?xml version='1.0' encoding='UTF-8'?>

<oor:component-data
	xmlns:oor="http://openoffice.org/2001/registry"
	xmlns:xs="http://www.w3.org/2001/XMLSchema"
	oor:name="%ExtensionName%Settings"
	oor:package="%prefix%">

	<node oor:name="ConfigNode">
		<prop oor:name="Origin" oor:type="xs:string">
			<value>%origin%</value>
		</prop>
		<prop oor:name="FirstRun" oor:type="xs:boolean">
			<value>true</value>
		</prop>
	</node>
</oor:component-data>

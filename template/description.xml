<?xml version="1.0" encoding="UTF-8"?>
<description xmlns="http://openoffice.org/extensions/description/2006"
	xmlns:xlink="http://www.w3.org/1999/xlink">

	<identifier value="%prefix%.%extensionname%"></identifier>

	<version value="1.0"></version>

	<registration>
		<simple-license  accept-by="admin" default-license-id="en" suppress-on-update="true">
			<license-text xlink:href="resources/license_en.txt" lang="en" license-id="en" />
		</simple-license>
	</registration>

	<display-name>
		<name lang="en">%Extension Name%</name>
	</display-name>

	<publisher>
		<name xlink:href="%url%" lang="en">%Vendor Name%</name>
	</publisher>

	<icon>
		<default xlink:href="extensionicon.png" />
	</icon>

</description>
